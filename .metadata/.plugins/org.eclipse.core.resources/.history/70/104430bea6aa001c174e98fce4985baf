package cj1;

import java.util.Scanner;

public class SwitchEx {

	public static void main(String[] args) {
		int n=5
		Switch(n)
		{

		case 1:System.out.println("oddnumber");
		break;
		case 2:System.out.println("oddnumber");
		break;
		case 3:System.out.println("oddnumber");
		break;
		case 4:System.out.println("oddnumber");
		break;
		case 5:System.out.println("oddnumber");
		break;
		}
		
	}

}
