package cj1;

public class ForLoop {
	public static void primeNumbers()
	{
		int count;
		int n;
		for(n=2;n<100;n++)
		{
			count=0;
			for(int i=1;i<=n;i++)
			{
					if(n%i==0)
					{
						count++;
					}
					
				}
			if(count==2) {
				System.out.println("prime number is ......."+n);
			}
		}
	public static void main(String[] args) {
     ForLoop.primeNumbers();
	}

}
