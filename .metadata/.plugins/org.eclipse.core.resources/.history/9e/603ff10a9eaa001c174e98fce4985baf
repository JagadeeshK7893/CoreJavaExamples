package cj1;

import java.util.Scanner;

public class LoopsandContinue {
	void oddNumbers()
	{
		Scanner ss=new Scanner(System.in);
		int limit=ss.nextInt();
	   for(int n=0;n<limt;n++)
	   {
		   int temp=n;
		   for(int i=0;i<n;i++)
		   {
			   if(temp%i==0)
			   {
				   continue;
			   }
			   System.out.println(n+".....is odd number");
		   }
	   }
	}

	public static void main(String[] args) {


	}

}
